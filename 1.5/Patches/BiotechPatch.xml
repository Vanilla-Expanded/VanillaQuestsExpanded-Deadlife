<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Biotech</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[defName = "VQED_MilitaryCrateOfHemogen"]/modExtensions/li[@Class="VanillaFurnitureExpanded.LootableBuildingDetails"]/contents/li[@Name="HemogenPacks"]</xpath>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName = "VQED_MilitaryCrateOfHemogen"]/modExtensions/li[@Class="VanillaFurnitureExpanded.LootableBuildingDetails"]/contents</xpath>
					<value>
						<li>
							<thing>HemogenPack</thing>
							<count>12</count>
						</li>
						
					</value>
				</li>
				
			</operations>
		</match>
	</Operation>
</Patch>

