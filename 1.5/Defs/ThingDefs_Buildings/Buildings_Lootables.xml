				<chanceNeverExplodeFromDamage>0</chanceNeverExplodeFromDamage>
				<startWickHitPointsPercent>0.2</startWickHitPointsPercent>
				<shell>Shell_HighExplosive</shell>
				<shellCount>4~4</shellCount>
				<shellDist>3~20</shellDist>
			</li>
		</comps>
	</ThingDef>
